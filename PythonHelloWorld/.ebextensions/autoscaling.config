option_settings:
  aws:autoscaling:asg:
    Availability Zones: Any
    Cooldown: '720'
    MaxSize: '2'
    MinSize: '1'
    EnableCapacityRebalancing: true
  aws:autoscaling:launchconfiguration:
    InstanceType: 't2.micro,t3.micro,t3.small'
    SecurityGroups: { "Fn::GetAtt" : [ "EC2SecurityGroup", "GroupId" ]}